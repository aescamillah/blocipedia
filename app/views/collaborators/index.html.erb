<h4>Add a collaborator:</h4>
</br>
<%= link_to "Add Collaborator", new_wiki_collaborator_path(@wiki.id), class: 'btn btn-primary' %>

<h2><p class="text-center">Collaborators</p></h2>
<div class="container">
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Remove</th>
      </tr>
    </thead>
    <tbody>
      <%= render partial: 'collaborators/collaborator', collection: @collaborators %>
    </tbody>
  </table>
</div>
